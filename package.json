{
  "name": "@zaboco/espm",
  "version": "0.1.0",
  "description": "ES Package Manager",
  "scripts": {
    "test": "uvu -r esbuild-runner/register -C packages",
    "test:watch": "FORCE_COLOR=1 watchlist -e packages -- npm test",
    "tw": "FORCE_COLOR=1 watchlist -e packages -- esr",
    "twd": "FORCE_COLOR=1 watchlist -e packages -- uvu -r esbuild-runner/register -C",
    "prepublishOnly": "esbuild --outfile=dist/cli.cjs --platform=node --bundle --format=cjs packages/cli/index.ts"
  },
  "files": [
    "package.json",
    "README.md",
    "dist/cli.cjs"
  ],
  "keywords": [],
  "author": "zaboco@gmail.com",
  "license": "MIT",
  "bin": "dist/cli.cjs",
  "devDependencies": {
    "@types/node": "16.11.22",
    "esbuild-runner": "2.2.1",
    "prettier": "2.5.1",
    "typescript": "4.5.5",
    "uvu": "0.5.3",
    "watchlist": "0.3.1"
  },
  "dependencies": {
    "@mobily/ts-belt": "3.9.0",
    "chalk": "2.4.2",
    "es-module-lexer": "0.10.0",
    "httpie": "1.1.2",
    "magic-string": "0.26.1",
    "spinnies": "0.5.1"
  }
}
